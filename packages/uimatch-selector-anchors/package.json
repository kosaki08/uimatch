{
  "name": "@uimatch/selector-anchors",
  "version": "0.1.0",
  "description": "Selector resolution plugin for uiMatch using AST-based anchors",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "uimatch-anchors": "./dist/cli/add-anchor.js"
  },
  "files": [
    "dist",
    "schema",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && npm run postbuild",
    "postbuild": "rimraf dist/__tests__ dist/**/*.test.* dist/**/safe-regex.test.*",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "bun test"
  },
  "keywords": [
    "uimatch",
    "selector",
    "ast",
    "anchors"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "jsep": "^1.4.0",
    "parse5": "^8.0.0",
    "safe-regex2": "^5.0.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "optionalDependencies": {
    "re2": "^1.21.4"
  },
  "peerDependencies": {
    "@uimatch/selector-spi": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@uimatch/selector-spi": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "@uimatch/selector-spi": "workspace:*",
    "rimraf": "^6.0.1"
  }
}
