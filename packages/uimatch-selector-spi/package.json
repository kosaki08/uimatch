{
  "name": "@uimatch/selector-spi",
  "version": "1.0.1",
  "description": "Service Provider Interface types for uiMatch selector resolution plugins",
  "type": "module",
  "typedoc": {
    "entryPoint": "src/index.ts"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "uimatch",
    "spi",
    "types",
    "selector"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3",
    "rimraf": "^6.0.1"
  }
}
