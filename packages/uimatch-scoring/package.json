{
  "name": "@uimatch/scoring",
  "version": "0.2.0",
  "type": "module",
  "license": "MIT",
  "description": "Design Fidelity Score (DFS) calculation for uiMatch",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typedoc": {
    "entryPoint": "src/index.ts"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "test": "bun test",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "design-fidelity",
    "scoring",
    "uimatch"
  ],
  "dependencies": {
    "@uimatch/core": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3",
    "rimraf": "^6.0.1"
  }
}
