{
  "name": "uimatch-scoring",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Design Fidelity Score (DFS) calculation for uiMatch",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "imports": {
    "#/*": "./dist/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "test": "bun test",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "design-fidelity",
    "scoring",
    "uimatch"
  ],
  "dependencies": {
    "uimatch-core": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
