    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "pixelmatch": "^6.0.0",
    "playwright": "^1.49.1",
    "pngjs": "^7.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/pixelmatch": "^5.2.6"
  }
